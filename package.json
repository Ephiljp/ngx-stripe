{
  "$schema": "./node_modules/ng-packagr/package.schema.json",
  "name": "@nomadreservations/ngx-stripe",
  "version": "1.1.0",
  "description": "@angular wrapper for StripeJS",
  "repository": {
    "type": "git",
    "url": "https://github.com/nomadreservations/ngx-stripe.git"
  },
  "bugs": {
    "url": "https://github.com/nomadreservations/ngx-stripe/issues"
  },
  "homepage": "https://github.com/nomadreservations/ngx-stripe",
  "keywords": [
    "angular",
    "javascript",
    "typescript"
  ],
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:release": "ng-packagr -p package.json",
    "release": "release-it",
    "release:major": "npm run release -- major -n",
    "release:minor": "npm run release -- minor -n",
    "release:patch": "npm run release -- patch -n",
    "compodoc": "compodoc -p tsconfig.json",
    "compodoc:serve": "compodoc -p tsconfig.json -sw",
    "compodoc:watch": "compodoc -p tsconfig.json -w",
    "docs": "npm-run-all -s compodoc:serve",
    "docs:watch": "npm-run-all -p compodoc:serve compodoc:watch",
    "docs:publish": "run-s demo:aot:yalc && bash deploy-docs.sh",
    "doctoc": "doctoc README.md --maxlevel 3"
  },
  "peerDependencies": {
    "@angular/common": "^7.0.0",
    "@angular/core": "^7.0.0"
  },
  "private": true,
  "dependencies": {},
  "devDependencies": {
    "@angular/animations": "~7.0.0",
    "@angular/cdk": "~7.1.0",
    "@angular/common": "~7.0.0",
    "@angular/compiler": "~7.0.0",
    "@angular/core": "~7.0.0",
    "@angular/forms": "~7.0.0",
    "@angular/http": "~7.0.0",
    "@angular/material": "^7.1.0",
    "@angular/platform-browser": "~7.0.0",
    "@angular/platform-browser-dynamic": "~7.0.0",
    "@angular/router": "~7.0.0",
    "core-js": "^2.5.4",
    "hammerjs": "^2.0.8",
    "mdi": "^2.2.43",
    "release-it": "^8.2.0",
    "rxjs": "~6.3.3",
    "zone.js": "~0.8.26",
    "@angular-devkit/build-angular": "~0.10.0",
    "@angular-devkit/build-ng-packagr": "~0.10.0",
    "@angular/cli": "~7.0.5",
    "@angular/compiler-cli": "~7.0.0",
    "@angular/language-service": "~7.0.0",
    "@compodoc/compodoc": "^1.1.7",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "doctoc": "^1.4.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^4.2.0",
    "npm-run-all": "^4.1.5",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.6"
  },
  "ngPackage": {
    "dest": "dist/ngx-stripe",
    "lib": {
      "entryFile": "projects/ngx-stripe/src/public_api.ts"
    }
  }
}
