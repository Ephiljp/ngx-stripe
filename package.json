{
  "name": "@nomadreservations/ngx-stripe",
  "version": "1.1.0",
  "description": "@angular wrapper for StripeJS",
  "repository": {
    "type": "git",
    "url": "https://github.com/nomadreservations/ngx-stripe.git"
  },
  "bugs": {
    "url": "https://github.com/nomadreservations/ngx-stripe/issues"
